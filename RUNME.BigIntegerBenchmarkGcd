#!/bin/bash

set -e

uname && sep=':' || sep=';'

./javacpp -Wall -Werror /usr/bin/javac -deprecation -deprecation -classpath ".:./donhatchsw.jar" BigIntegerBenchmarkGcd.prejava 

echo java -Xmx10g -classpath ".${sep}../donhatchsw" BigIntegerBenchmarkGcd "$@"
exec java -Xmx10g -classpath ".${sep}../donhatchsw" BigIntegerBenchmarkGcd "$@"

